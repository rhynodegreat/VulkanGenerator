<?xml version="1.0" encoding="utf-8" ?>
<patch>
  <structs>
    <struct name="VkAllocationCallbacks">
      <field name="pUserData" type="IntPtr" />
      <field name="pfnAllocation" type="IntPtr" />
      <field name="pfnReallocation" type="IntPtr" />
      <field name="pfnFree" type="IntPtr" />
      <field name="pfnInternalAllocation" type="IntPtr" />
      <field name="pfnInternalFree" type="IntPtr" />
    </struct>
    <struct name="VkPhysicalDeviceProperties">
      <field name="deviceName" type="byte[]" marshal-type="ByValArray" marshal-size="256"/>
      <field name="pipelineCacheUUID" type="byte[]" marshal-type="ByValArray" marshal-size="16"/>
    </struct>
    <struct name="VkExtensionProperties">
      <field name="extensionName" type="byte[]" marshal-type="ByValArray" marshal-size="256"/>
      <field name="specVersion" type="VkVersion" />
    </struct>
    <struct name="VkLayerProperties">
      <field name="layerName" type="byte[]" marshal-type="ByValArray" marshal-size="256"/>
      <field name="specVersion" type="VkVersion" />
      <field name="description" type="byte[]" marshal-type="ByValArray" marshal-size="256"/>
    </struct>
    <struct name="VkApplicationInfo">
      <field name="pApplicationName" type="byte[]" marshal-type="LPArray"/>
      <field name="pEngineName" type="byte[]" marshal-type="LPArray"/>
      <field name="applicationVersion" type="VkVersion" />
      <field name="engineVersion" type="VkVersion" />
      <field name="apiVersion" type="VkVersion" />
    </struct>
    <struct name="VkDeviceQueueCreateInfo">
      <field name="pQueuePriorities" type="float[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDeviceCreateInfo">
      <field name="pQueueCreateInfos" type="VkDeviceQueueCreateInfo[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkInstanceCreateInfo">
      <field name="pApplicationInfo" type="IntPtr"/>
    </struct>
    <struct name="VkPhysicalDeviceMemoryProperties">
      <field name="memoryTypes" type="VkMemoryType" marshal-type="ByValArray" marshal-size="32"/>
      <field name="memoryHeaps" type="VkMemoryHeap" marshal-type="ByValArray" marshal-size="16"/>
    </struct>
    <struct name="VkBufferCreateInfo">
      <field name="pQueueFamilyIndices" type="uint[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkImageCreateInfo">
      <field name="pQueueFamilyIndices" type="uint[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkBindSparseInfo">
      <field name="pWaitSemaphores" type="VkSemaphore[]" marshal-type ="LPArray"/>
      <field name="pBufferBinds" type="VkSparseBufferMemoryBindInfo[]" marshal-type="LPArray"/>
      <field name="pImageOpaqueBinds" type="VkSparseImageOpaqueMemoryBindInfo[]" marshal-type="LPArray"/>
      <field name="pImageBinds" type="VkSparseImageMemoryBindInfo[]" marshal-type="LPArray"/>
      <field name="pSignalSemaphores" type="VkSemaphore" marshaltype="LPArray"/>
    </struct>
    <struct name="VkShaderModuleCreateInfo">
      <field name="pCode" type="uint[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDescriptorSetLayoutBinding">
      <field name="pImmutableSamplers" type="VkSampler[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDescriptorSetLayoutCreateInfo">
      <field name="pBindings" type="VkDescriptorSetLayoutBinding[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDescriptorPoolCreateInfo">
      <field name="pPoolSizes" type="VkDescriptorPoolSize[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDescriptorSetAllocateInfo">
      <field name="pSetLayouts" type="VkDescriptorSetLayout[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPipelineShaderStageCreateInfo">
      <field name="pSpecializationInfo" type="IntPtr"/>
    </struct>
    <struct name="VkPipelineVertexInputStateCreateInfo">
      <field name="pVertexBindingDescriptions" type="VkVertexInputBindingDescription[]" marshal-type="LPArray"/>
      <field name="pVertexAttributeDescriptions" type="VkVertexInputAttributeDescription[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPipelineViewportStateCreateInfo">
      <field name="pViewports" type="VkViewport[]" marshal-type="LPArray"/>
      <field name="pScissors" type="VkRect2D[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPipelineMultisampleStateCreateInfo">
      <field name="pSampleMask" type="VkSampleMask[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPipelineColorBlendStateCreateInfo">
      <field name="pAttachments" type="VkPipelineColorBlendAttachmentState" marshal-type="LPArray"/>
      <field name="blendConstants" type="float[]" marshal-type="ByValArray" marshal-size="4"/>
    </struct>
    <struct name="VkPipelineDynamicStateCreateInfo">
      <field name="pDynamicStates" type="VkDynamicState[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkGraphicsPipelineCreateInfo">
      <field name="pStages" type="VkPipelineShaderStageCreateInfo[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPipelineLayoutCreateInfo">
      <field name="pSetLayouts" type="VkDescriptorSetLayout" marshal-type="LPArray"/>
      <field name="pPushConstantRanges" type="VkPushConstantRange[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkRenderPassBeginInfo">
      <field name="pClearValues" type="VkClearValue[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkSubpassDescription">
      <field name="pInputAttachments" type="VkAttachmentReference[]" marshal-type="LPArray"/>
      <field name="pColorAttachments" type="VkAttachmentReference[]" marshal-type="LPArray"/>
      <field name="pResolveAttachments" type="VkAttachmentReference[]" marshal-type="LPArray"/>
      <field name="pPreserveAttachments" type="uint[]" marshal-type="LPArray"/>
      <field name="pDepthStencilAttachment" type="IntPtr"/>
    </struct>
    <struct name="VkRenderPassCreateInfo">
      <field name="pAttachments" type="VkAttachmentDescription[]" marshal-type="LPArray"/>
      <field name="pSubpasses" type="VkSubpassDescription[]" marshal-type="LPArray"/>
      <field name="pDependencies" type="VkSubpassDependency[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkFramebufferCreateInfo">
      <field name="pAttachments" type="VkImageView" marshal-type="LPArray"/>
    </struct>
    <struct name="VkSubmitInfo">
      <field name="pWaitSemaphores" type="VkSemaphore[]" marshal-type="LPArray"/>
      <field name="pWaitDstStageMask" type="VkPipelineStageFlags[]" marshal-type="LPArray"/>
      <field name="pCommandBuffers" type="VkCommandBuffer[]" marshal-type="LPArray"/>
      <field name="pSignalSemaphores" type="VkSemaphore[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDisplayPropertiesKHR">
      <field name="displayName" type="byte[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkSwapchainCreateInfoKHR">
      <field name="pQueueFamilyIndices" type="uint[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkPresentInfoKHR">
      <field name="pWaitSemaphores" type="VkSemaphore[]" marshal-type="LPArray"/>
      <field name="pSwapchains" type="VkSwapchainKHR[]" marshal-type="LPArray"/>
      <field name="pImageIndices" type="uint[]" marshal-type="LPArray"/>
      <field name="pResults" type="VkResult[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDebugMarkerObjectNameInfoEXT">
      <field name="pObjectName" type="byte[]" marshal-type="LPArray"/>
    </struct>
    <struct name="VkDebugMarkerMarkerInfoEXT">
      <field name="pMarkerName" type="byte[]" marshal-type="LPArray"/>
      <field name="color" type="float[]" marshal-type="ByValArray" marshal-size="4"/>
    </struct>
  </structs>
  <commands>
    <params>
      <preserve type="VkAllocationCallbacks*"/>
    </params>
  </commands>
</patch>